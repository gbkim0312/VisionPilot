{
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.trimTrailingWhitespace": true,
    "clangd.enable": true,
    "cmake.generator": "Ninja",
    "cmake.configureOnOpen": false,
    "cmake.configureOnEdit": false,
    "cmake.buildDirectory": "${workspaceFolder}/build/debug/vision_pilot",
    "cmake.statusbar.visibility": "compact",
    "C_Cpp.intelliSenseEngine": "disabled",
    "clang-format.executable": "/usr/bin/clang-format",
    "clang-format.style": "${workspaceFolder}/.clang-format",
    "clang-format.language.c.enable": true,
    "clang-format.language.cpp.enable": true,
    "cmake.parallelJobs": 0,
    "cmake.statusbar.advanced": {
        "kit": {
            "visibility": "hidden"
        },
        "ctest": {
            "visibility": "hidden"
        },
        "status": {
            "visibility": "hidden"
        },
        "launch": {
            "visibility": "hidden"
        }
    },
    "cmake.debugConfig": {
        "cwd": "${command:cmake.buildDirectory}/test",
        "args": [
            // "--gtest_filter=TestPolicyReqCertGen.\\*",
            // "--gtest_repeat=10"
            // "--help",
            // "-o",
            // "\"get http://google.co.kr\""
        ]
    },
    "cmake.configureArgs": [
        "-DCMAKE_BUILD_TYPE=Debug",
        "-DCMAKE_INSTALL_PREFIX=${workspaceFolder}/build/debug/install",
